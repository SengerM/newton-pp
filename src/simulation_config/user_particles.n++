vector<Particle> particles(N_PARTICLES);

for (j=0; j<N_PARTICLES; j++) {
	particles[j] = Particle();
	particles[j].Mass() = PARTICLES_MASS;
	particles[j].Position() = Vec3D(
		random_double(-sqrt(INITIAL_VOLUME)/2,sqrt(INITIAL_VOLUME)/2),
		random_double(-sqrt(INITIAL_VOLUME)/2,sqrt(INITIAL_VOLUME)/2),
		0);
}

